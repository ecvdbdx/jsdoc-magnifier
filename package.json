{
    "name": "jsdoc-magnifier",
    "version": "0.0.0",
    "description": "A project to learn node",
    "main": "index.js",
    "type": "module",
    "bin": {
        "jsdoc-magnifier": "./cli.js"
    },
    "scripts": {
        "test": "node --test",
        "prepare": "husky",
        "lint": "npx eslint \"**/*.js\"",
        "prettier": "prettier ./ --write",
        "start:api": "tsx --env-file=.env packages/api/server.ts",
        "dev:api": "tsx watch --env-file=.env packages/api/server.ts",
        "test:api": "node --env-file=.env api/index.test.js"
    },
    "author": "ECV M2 Dev",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "eslint": "^9.13.0",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "tsx": "^4.19.2"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@hono/node-server": "^1.13.7",
        "commander": "^12.1.0",
        "hono": "^4.6.10"
    }
